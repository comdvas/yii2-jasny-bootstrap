{
        "name": "comdvas/yii2-jasny-bootstrap",
        "description": "Extension for Yii 2 to work with Jasny Bootstrap",
        "keywords": ["yii2", "extension", "jasny", "bootstrap"],
        "type": "yii2-extension",
        "license": "BSD-3-Clause",
        "support": {
                "issues": "https://github.com/comdvas/yii2-jasny-bootstrap/issues",
                "source": "https://github.com/comdvas/yii2-jasny-bootstrap"
        },
        "minimum-stability": "dev",
        "authors": [
            {
                "name": "Evgeny Vasiliev"
            }
        ],
        "require": {
            "yiisoft/yii2": "*",
            "bower-asset/jasny-bootstrap": "*"
        },    
        "autoload": {
            "psr-4": { "comdvas\\jasnybootstrap\\": "" }
        }
}
